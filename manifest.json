{
  "manifest_version": 3,
  "name": "Timer Extension",
  "version": "1.0",
  "description": "A Chrome extension that displays a timer.",
  "permissions": [
    "alarms",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
